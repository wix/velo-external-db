  "main": "lib/app.js",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "files": [ "lib" ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix/velo-external-db.git",
    "directory": "packages/velo-external-db"
  },
  "bugs": {
    "url": "https://github.com/wix/velo-external-db/issues"
